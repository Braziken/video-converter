{
  "name": "video-converter",
  "version": "1.0.0",
  "productName": "Online Video Converter",
  "description": "Applicaton to convert and download [YouTube or Dailymotion] videos as MP3s or MP4s.",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Youssef Benhessou",
    "url": "https://github.com/Youssef-ben/video-converter"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "cross-env BROWSER='none' react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "run-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "start-electron": "electron .",
    "pack-win-electron": "yarn build & electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never",
    "postinstall-electron": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "about-window": "^1.13.2",
    "bootstrap": "^4.4.1",
    "electron-is-dev": "^1.2.0",
    "ffmpeg-static": "^4.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^9.0.0",
    "https-proxy-agent": "^5.0.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-intl": "^4.3.1",
    "react-scripts": "3.4.1",
    "sanitize-filename": "^1.6.3",
    "ytdl-core": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "electron": "^8.2.1",
    "electron-builder": "^22.4.1",
    "html-webpack-plugin": "^4.0.4",
    "node-sass": "^4.13.1",
    "wait-on": "^4.0.2"
  },
  "build": {
    "appId": "com.video.converter",
    "productName": "Video Converter",
    "icon": "build/logo512.png",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
